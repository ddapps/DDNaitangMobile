#import "NT_BaseAppInfo.h"

@implementation NT_BaseAppInfo

+ (NT_BaseAppInfo *)inforFromDetailArray:(NSMutableArray *)array
{
    NT_BaseAppInfo *info = [[NT_BaseAppInfo alloc] init];
    info.selected = NO;
    info.selectIndex = -1;
    info.infoArray = array;
    return info;
}

@end
